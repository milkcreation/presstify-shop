{
  "name": "presstify-plugins/shop",
  "description": "Extension PresstiFy de gestion de boutique en ligne.",
  "type": "presstify-plugin",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "e-commerce",
    "woocommerce"
  ],
  "homepage": "https://svn.milkcreation.fr/presstify-plugins/shop",
  "time": "2018-09-14",
  "license": "proprietary",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.tigreblanc.fr/presstify/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "presstify/framework": "^2.0"
  },
  "autoload": {
    "files": [
      "Helpers.php"
    ],
    "psr-4": {
      "tiFy\\Plugins\\Shop\\": ""
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config" : {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
